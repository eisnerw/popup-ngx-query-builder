{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-query-builder": {
      "projectType": "library",
      "root": "projects/ngx-query-builder",
      "sourceRoot": "projects/ngx-query-builder/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-query-builder/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-query-builder/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-query-builder/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-query-builder/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "popup-ngx-query-builder": {
      "projectType": "library",
      "root": "projects/popup-ngx-query-builder",
      "sourceRoot": "projects/popup-ngx-query-builder/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/popup-ngx-query-builder/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/popup-ngx-query-builder/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/popup-ngx-query-builder/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/popup-ngx-query-builder/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "popup-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/popup-demo",
      "sourceRoot": "projects/popup-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/popup-demo",
            "index": "projects/popup-demo/src/index.html",
            "browser": "projects/popup-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/popup-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/popup-demo/public"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "projects/popup-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "3MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kB",
                  "maximumError": "12kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "popup-demo:build:production"
            },
            "development": {
              "buildTarget": "popup-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/popup-demo/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/popup-demo/public"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "projects/popup-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
